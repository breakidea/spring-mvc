#startup()
<!doctype html>
<html>
<head>
<title>sample/index</title>
<link rel="stylesheet" href="$context.getContextUrl('/static/sample.css')">
</head>
<body>
    <table class="c-table" border="1" cellspacing="1" cellpadding="2">
        <tr>
            <td width="80">getGuid:</td>
            <td data-value="[\w-]{36}">$util.getGuid()</td>
        </tr>
        <tr>
            <td>获取配置</td>
            <td data-value="ionnic">$page.config("domain")</td>
        </tr>
        <tr>
            <td>外部模板</td>
            <td data-value="AD">
                <pre>$page.external("ad")</pre>
                <pre>$page.external("inc", {"status": 0})</pre>
            </td>
        </tr>
        <tr>
            <td>内部模板</td>
            <td data-value="[\S]{1,}">
                <pre>#includeInternal("ad")</pre>
                <pre>#includeInternal("ad", {})</pre>
            </td>
        </tr>
    </table>
