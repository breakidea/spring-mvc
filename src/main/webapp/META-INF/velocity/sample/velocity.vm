#startup()
<!doctype html>
<html>
<head>
<title>sample/index</title>
<link rel="stylesheet" href="$context.getContextUrl('/static/sample.css')">
</head>
<body>
    <table class="c-table" border="1" cellspacing="1" cellpadding="2">
        <tr>
            <td width="80">getGuid:</td>
            <td data-value="[\w-]{36}">$util.getGuid()</td>
        </tr>
        <tr>
            <td>配置项:</td>
            <td data-value="ionnic">
                $page.config("domain")
            </td>
        </tr>
        <tr>
            <td>上下文:</td>
            <td data-value="ionnic">
                $context.getRequestUri()
            </td>
        </tr>
        <tr>
            <td>外部模板:</td>
            <td data-value="[\S]{1,}">#includeExternal("ad")</td>
        </tr>
    </table>
    <pre>
    $page.config("domain")
    $page.external("ad", {})
    
    $page.eval(
        '$page.config("domain")#foreach($item in $array)\t<a href="javascript:alert(\"$velocityCount:\")">$name</a>($util.toLocateDate("default"))\n#end', {
        "array": [1,2,3,4],
        "name" : "index"
    })
    </pre>
    ---------
    $context
    $page
    $response
    $session
    $application
    
    <script src="$context.getContextUrl('/static/sample.js')"></script>
</body>
</html>