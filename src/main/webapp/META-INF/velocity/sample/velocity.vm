#startup()
<!doctype html>
<html>
<head>
<title>sample/index</title>
<link rel="stylesheet" href="$context.getContextUrl('/static/sample.css')">
</head>
##block("end")
<body>
    <table class="c-table" border="1" cellspacing="1" cellpadding="2">
        <tr>
            <td width="80">getGuid:</td>
            <td data-value="[\w-]{36}">$util.getGuid()</td>
        </tr>
        <tr>
            <td>配置项:</td>
            <td data-value="ionnic">
                $page.getConfig("domain"), $page.mail
            </td>
        </tr>
        <tr>
            <td>上下文:</td>
            <td data-value="ionnic">
                $context.getRequestUri()
            </td>
        </tr>
        <tr>
            <td>外部模板:</td>
            <td data-value="[\S]{1,}">#includeExternal("ad")</td>
        </tr>
    </table>
    
    
    $util
    
    
    $page.getConfig("domain")
    $page.external("ad")
    $page.eval(
        '#foreach($item in $array)\n\n$util.getDate("now")\n#end', {
        "array": [1,2,3,4],
        "name" : 12
    })
    
    ##$page.getResponse().getWriter().append("2")
    
    ## $request
    ## $context.getWebApplicationContext().getNamespace()
    ## $request.getSession()
    <script src="$context.getContextUrl('/static/sample.js')"></script>
</body>
</html>