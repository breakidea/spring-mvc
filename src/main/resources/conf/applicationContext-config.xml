<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"    xmlns:cache="http://www.springframework.org/schema/bean"    xsi:schemaLocation="            http://www.springframework.org/schema/beans             http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">        <!-- 全局配置 -->    <bean class="org.ionnic.core.support.Config">        <constructor-arg>            <bean class="org.ionnic.core.support.config.InitWrapper">                <property name="encoding" value="UTF-8" />                <property name="viewConfig">                    <list>                        <value>/WEB-INF/local/conf.properties</value>                    </list>                </property>            </bean>        </constructor-arg>    </bean></beans>