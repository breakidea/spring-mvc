<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    xmlns:util="http://www.springframework.org/schema/util"    xsi:schemaLocation="        http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">    <bean class="org.ionnic.common.web.UriLandingServer">        <property name="landingUrl">            <map>                <entry key="imageServer" value="http://www.img.com"/>                <entry key="loginServer" value="member/signin.htm"/>            </map>        </property>    </bean>    <bean id="map" class="org.springframework.beans.factory.config.MapFactoryBean">        <property name="targetMapClass" value="java.util.HashMap"/>        <property name="sourceMap">            <map>                <entry key="1" value="first"/>                <entry key="2" value="second"/>                <entry key="3" value="three"/>                <entry key="4" value="four"/>            </map>        </property>    </bean></beans>